{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  name: 'HomeComponent',\n  data() {\n    return {\n      newProducts: [],\n      discountedProducts: []\n    };\n  },\n  computed: {\n    ...mapGetters([\"products\", \"wishlist\"])\n  },\n  methods: {\n    returnImgSrc(src) {\n      return src + '_large.png';\n    },\n    makeRating: function (rating) {\n      let html = '';\n      if (rating == null) {\n        html += `\n\t\t\t\t<i class=\"fa fa-star-o\"></i>\n\t\t\t\t<i class=\"fa fa-star-o\"></i>\n\t\t\t\t<i class=\"fa fa-star-o\"></i>\n\t\t\t\t<i class=\"fa fa-star-o\"></i>\n\t\t\t\t<i class=\"fa fa-star-o\"></i>\n\t\t\t\t`;\n      } else {\n        for (let i = 0; i < rating; i++) {\n          html += `<i class=\"fa fa-star\"></i>`;\n        }\n        for (let i = 0; i < 5 - rating; i++) {\n          html += `<i class=\"fa fa-star-o\"></i>`;\n        }\n      }\n      return html;\n    },\n    makeLinkForSingle: function (name) {\n      return \"/product/\" + name;\n    },\n    checkProductInWishlist(id) {\n      for (let product of this.wishlist) {\n        if (product.id == id) {\n          return \"add-to-wishlist active\";\n        }\n      }\n      return \"add-to-wishlist\";\n    },\n    checkProductForIcon(id) {\n      for (let product of this.wishlist) {\n        if (product.id == id) {\n          return `fa fa-heart`;\n        }\n      }\n      return `fa fa-heart-o`;\n    },\n    addToWishlist: function (id) {\n      let button = document.querySelector('.add-to-wishlist[data-id=\"' + id + '\"]');\n      if (button.classList.contains('active')) {\n        button.classList.remove('active');\n        button.innerHTML = `<i class=\"fa fa-heart-o\"></i>`;\n        this.$store.commit('removeProductFromWishlist', id);\n      } else {\n        button.classList.add('active');\n        button.innerHTML = `<i class=\"fa fa-heart\"></i>`;\n        this.$store.commit(\"addProductToWishlist\", {\n          id: id\n        });\n      }\n    },\n    addToCart: function (p) {\n      this.$store.commit(\"addProductToCart\", {\n        product: p,\n        SingleProduct: false\n      });\n    }\n  },\n  mounted() {\n    setTimeout(function () {\n      $('footer script').each(function () {\n        $(this).remove();\n      });\n      let Script2 = document.createElement(\"script\");\n      Script2.setAttribute(\"src\", \"assets/js/slick.min.js\");\n      document.head.appendChild(Script2);\n    }, 100);\n    setTimeout(function () {\n      let Script = document.createElement(\"script\");\n      Script.setAttribute(\"src\", \"assets/js/mainold.js\");\n      document.querySelector('footer').appendChild(Script);\n    }, 200);\n    this.newProducts = this.products.sort((a, b) => {\n      return b.product_id - a.product_id;\n    }).slice(0, 8);\n    this.discountedProducts = this.products.filter(x => x.discount_id != null).slice(0, 8);\n  }\n};","map":{"version":3,"names":["mapGetters","name","data","newProducts","discountedProducts","computed","methods","returnImgSrc","src","makeRating","rating","html","i","makeLinkForSingle","checkProductInWishlist","id","product","wishlist","checkProductForIcon","addToWishlist","button","document","querySelector","classList","contains","remove","innerHTML","$store","commit","add","addToCart","p","SingleProduct","mounted","setTimeout","$","each","Script2","createElement","setAttribute","head","appendChild","Script","products","sort","a","b","product_id","slice","filter","x","discount_id"],"sources":["src/components/pages/Main.vue"],"sourcesContent":["<template>\r\n    <div>\r\n\t\t<div class=\"section\">\r\n\t\t\t<!-- container -->\r\n\t\t\t<div class=\"container\">\r\n\t\t\t\t<!-- row -->\r\n\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t<!-- shop -->\r\n\t\t\t\t\t<div class=\"col-md-4 col-xs-6\">\r\n\t\t\t\t\t\t<div class=\"shop\">\r\n\t\t\t\t\t\t\t<div class=\"shop-img\">\r\n\t\t\t\t\t\t\t\t<img src=\"assets/img/shop01.png\" alt=\"aa\">\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t